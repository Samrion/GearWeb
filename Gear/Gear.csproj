      <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="Management\UI\Views\GearConfigPanel.cshtml">
      <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
