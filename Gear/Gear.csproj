<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Management\UI\Scripts\main.js" />
    <None Remove="Management\UI\Views\GearConfigLayout.cshtml" />
    <None Remove="Management\UI\Views\GearConfigPanel.cshtml" />
  </ItemGroup>



  <ItemGroup>
    <EmbeddedResource Include="Management\UI\Scripts\angular.js" />
    <EmbeddedResource Include="Management\UI\Scripts\main.js" />
